<h3 class="text-center">Objects</h3>

<% for(obj of imgAnnotated.cogniAPI.objects){ %>
    <div    class="mt-3 mb-3"
            onmouseover="boundFace(
                'face_box_<%= imgAnnotated.imgName%>', 1, '<%= imgAnnotated.cogniAPI.metadata.width%>', '<%= imgAnnotated.cogniAPI.metadata.height%>',
                '<%=obj.boundingBox.tl.y%>', '<%=obj.boundingBox.tl.x%>',
                '<%=obj.boundingBox.width%>', '<%=obj.boundingBox.height%>',
                )"
                onmouseleave="eraseFaceBound('face_box_<%= imgAnnotated.imgName%>')">
        <%=obj.name%>
        <div class="progress">
            <div class="progress-bar bg-primary" role="progressbar" style="width: <%= Math.floor(obj.confidence*100)%>%>%;"
                 aria-valuenow="<%= Math.floor(obj.confidence*100)%>" aria-valuemin="0" aria-valuemax="100">
                <%= Math.floor(obj.confidence*100)%>%
            </div>
        </div>
    </div>
<% }%>