<!--IMG BOX-->
<div class="row mt-3">

    <div id="img_box_<%= imgAnnotated.imgName%>" class="col-6">
        <img onload="fixHeights('<%= imgAnnotated.imgName%>')" id="img_<%= imgAnnotated.imgName%>" class="w-100 h-auto ml-auto mr-auto" src="<%= imgAnnotated.imgUrl%>">
        <div class="faceBoundingBox" id="face_box_<%= imgAnnotated.imgName%>"></div>
    </div>


    <div id="img_ann_box_<%= imgAnnotated.imgName%>" class="col-6 alert-info" style="display:none;overflow: scroll">

        <div id="FaceDetRec_<%= imgAnnotated.imgName%>">
            <%- include('faceDetRec', imgAnnotated)%>
        </div>

        <div id="Faces_<%= imgAnnotated.imgName%>" style="display: none">
            <%- include('faces', imgAnnotated)%>
        </div>

        <div id="Description_<%= imgAnnotated.imgName%>" style="display: none">
            <%- include('description', imgAnnotated)%>
        </div>

        <div id="Tags_<%= imgAnnotated.imgName%>" style="display: none">
            <%- include('tags', imgAnnotated)%>
        </div>

        <div id="Objects_<%= imgAnnotated.imgName%>" style="display: none">
            <%- include('objects', imgAnnotated)%>
        </div>

        <div id="Text_<%= imgAnnotated.imgName%>" style="display: none">
            <%- include('text', imgAnnotated)%>
        </div>

        <div id="Landmarks_<%= imgAnnotated.imgName%>" style="display: none">
            <%- include('landmarks', imgAnnotated)%>
        </div>

        <div id="SafetyAnn_<%= imgAnnotated.imgName%>" style="display: none">
            <%- include('safetyAnn', imgAnnotated)%>
        </div>

        <div id="Colors_<%= imgAnnotated.imgName%>" style="display: none">
            <%- include('colors', imgAnnotated)%>
        </div>

    </div>
</div>