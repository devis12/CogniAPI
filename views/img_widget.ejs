<div class="text-center">
    <nav class="tabs">
        <div class="selector"></div>
        <a class="active" onclick="changePaperTab('FaceDetRec_', '<%= imgAnnotated.imgName%>')">
            Face Detection & Recognition
        </a>
        <a onclick="changePaperTab('Faces_', '<%= imgAnnotated.imgName%>')">
            Faces
        </a>
        <a onclick="changePaperTab('Description_', '<%= imgAnnotated.imgName%>')">
            Description
        </a>
        <a onclick="changePaperTab('Tags_', '<%= imgAnnotated.imgName%>')">
            Tags
        </a>
        <a onclick="changePaperTab('Objects_', '<%= imgAnnotated.imgName%>')">
            Objects
        </a>
        <a onclick="changePaperTab('Text_', '<%= imgAnnotated.imgName%>')">
            Text
        </a>
        <a onclick="changePaperTab('Landmarks_', '<%= imgAnnotated.imgName%>')">
            Landmarks
        </a>
        <a onclick="changePaperTab('SafetyAnn_', '<%= imgAnnotated.imgName%>')">
            Safety
        </a>
        <a onclick="changePaperTab('Colors_', '<%= imgAnnotated.imgName%>')">
            Colors
        </a>
    </nav>

